package game.engine;

public class GameObject {

	private float x, y, z, w;
	private float sizex, sizey, sizez, sizew;
	
	public GameObject(float x, float y, float z, float w, 
					  float sizex, float sizey, float sizez, float sizew) {
		
		this.x = x;
		this.y = y;
		this.z = z;
		this.w = w;
		this.sizex = sizex;
		this.sizey = sizey;
		this.sizez = sizez;
		this.sizew = sizew;
	}
	
	public GameObject(float x, float y, float z, float w, float size) {
		
		this.x = x;
		this.y = y;
		this.z = z;
		this.w = w;
		this.sizex = size;
		this.sizey = size;
		this.sizez = size;
		this.sizew = size;
	}
}
