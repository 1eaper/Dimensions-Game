package game;

import game.engine.GameObject;

public class Player extends GameObject{

	public Player() {
		
		super(50, 50, 50, 50, 50);
	}
	
	public Player(float x, float y, float z, float w, float size) {
		
		super(x, y, z, w, size);
	}
	
	public void update() {
		
		
	}
	
	public void render() {
		
		
	}
	
	public void move2d(float velx, float vely) {
		
		x += velx;
		y += vely;
	}
}
